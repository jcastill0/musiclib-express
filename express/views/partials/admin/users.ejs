    <div class="container" ng-controller="UserCtrl">
      <div class="row">
	<div class="span1">
	</div>
	<div class="span9">
	  <h2>Users</h2>
	</div>
	<div class="span2">
	</div>
      </div>
      <div class="row">
	<div class="span1">
	</div>
	<div class="span11">
	  <input type="text" ng-model="searchUser" class="search-query" placeholder="Search"/>
	</div>
      </div>
      <div class="row" ng-repeat="user in users | filter:searchUser | orderBy:'email'">
	<div class="span4">
	  {{user.google_email}}
	</div>
	<div class="span4">
	  {{user.google_display_name}}
	</div>
	<div class="span4">
	  {{user.last_login}}
	</div>
      </div>
    </div>

