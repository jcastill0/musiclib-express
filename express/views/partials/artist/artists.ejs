    <div class="container" ng-controller="ArtistCtrl">
      <div class="row">
	<div class="col-md-1">
	</div>
	<div class="col-md-9">
	  <h2>Artists</h2>
	</div>
	<div class="col-md-2">
	  <a ng-href="#/playlists">[Back to Playlists]</a>
	</div>
      </div>
      <div class="row">
	<div class="col-md-1">
	</div>
	<div class="col-md-11">
	  <input type="search" ng-model="searchArtist" class="rounded search-query" placeholder="Search"/>
	</div>
      </div>
      <div class="row" ng-repeat="artist in artists | filter:searchArtist | orderBy:'name'">
	<div class="col-md-1">
	</div>
	<div class="col-md-11">
	  <a ng-href="#/artists/{{artist.id}}">{{artist.name}}</a>
	</div>
      </div>
    </div>


