  <div class="container" ng-controller="VideoListCtrl">
    <div class="row">
      <div class="span1">
      </div>
      <div class="span5">
	<h2>Videos
	  <a class="btn btn-small btn-info" ng-href="#/videos/add"><i class="icon-plus-sign"></i></a>
	</h2>
      </div>
      <div class="span6">
      </div>
    </div>
    <div class="row" ng-repeat="video in videos">
      <div class="span1">
      </div>
      <div class="span11">
	<a ng-href="{{video.url}}">{{video.name}}</a>
      </div>
      <hr/>
    </div>
  </div>

