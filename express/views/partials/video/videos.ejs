  <div class="container" ng-controller="VideoCtrl">
    <div class="row">
      <div class="span1">
      </div>
      <div class="span5">
	<h2>Videos
	  <a class="btn btn-small btn-info" ng-href="#/videos/add" data-toggle="tooltip" title="Add"><i class="icon-plus-sign"></i></a>
	</h2>
      </div>
      <div class="span6">
      </div>
    </div>
    <div class="row" ng-repeat="video in videos">
      <div class="span1">
      </div>
      <div class="span10">
	<a ng-href="{{video.url}}" target="_blank">{{video.name}}</a>
      </div>
      <div class="span1">
	<a class="btn btn-small btn-danger" ng-href="#/videos/delete/{{video.id}}" data-toggle="tooltip" title="Delete" id="DeleteActionID" ng-show="su" onClick="return confirm('Are you sure you want to delete this video?');"><i class="icon-remove icon-red"></i></a>
      </div>
      <hr/>
    </div>
  </div>

